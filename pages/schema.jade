extends ./layout

block link
  -var selected = 'Schema';
block content    
    // Counts Section
    .pagebox    
        section.dashboard-counts
            header
                .boxdisplay
                    .largetitle.pull-left Schema
                    .wrapper.count-title.d-flex.pull-right
                        i.fa.fa-search(aria-hidden='true')
                        .name.ml-3
                            input(type='text')
        section
            .clear.pt-3
                .row.text-center.m-3
                    .col-md-4
                        a.btn-main-big(href='sqlsearchchart.html', data-toggle='modal', data-target='#addtableModal')
                            i.fa.fa-plus(aria-hidden='true')
                            |  Schema/Tables
                    // Modal Add schema
                    #addschemaModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalCenterTitle', aria-hidden='true')
                        .modal-dialog.modal-dialog-centered(role='document')
                            .modal-content
                                .modal-header
                                    #exampleModalLongTitle.modal-title.text-white Add Schema
                                    button.close(type='button', data-dismiss='modal', aria-label='Close')
                                        i.fa.fa-times.text-red
                                .modal-body.text-white
                                    .row
                                        // Count item widget
                                        .col-md-12
                                            .btn-group(style='float:left')
                                                span.minwidth150.mr-2 Schema
                                                button.btn.dropdown-toggle.minwidthcombobox.minwidth250(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Select Schema
                                                .dropdown-menu
                                                    a.dropdown-item(href='#') Schema 1
                                                    a.dropdown-item(href='#') Schema 2
                                                    a.dropdown-item(href='#') Schema 3
                                            .btn-group.clear.mt-2(style='float:left;')
                                                span.minwidth150.mr-2 Name
                                                input.minwidth250(type='text')
                                .modal-footer
                                    .col-md-12.p-0
                                        button.btn.btn-mainadd(type='button', style='min-width:150px;', data-toggle='dropdown', data-dismiss='modal', aria-haspopup='true', aria-expanded='false')
                                            | Add
                    // Modal Add schema
                    #addtableModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalCenterTitle', aria-hidden='true')
                        .modal-dialog.modal-dialog-centered(role='document')
                            .modal-content
                                .modal-header
                                    #exampleModalLongTitle.modal-title.text-white Add Table
                                    button.close(type='button', data-dismiss='modal', aria-label='Close')
                                        i.fa.fa-times.text-red
                                .modal-body.text-white
                                    .row
                                        // Count item widget
                                        .col-md-12
                                            .btn-group(style='float:left')
                                                span.minwidth150.mr-2 Add
                                                button.btn.dropdown-toggle.minwidthcombobox.minwidth250(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Table
                                                .dropdown-menu
                                                    a.dropdown-item(href='#') Schema 1
                                                    a.dropdown-item(href='#') Schema 2
                                                    a.dropdown-item(href='#') Schema 3
                                            .btn-group.clear.mt-2(style='float:left')
                                                span.minwidth150.mr-2 Select
                                                button.btn.dropdown-toggle.minwidthcombobox.minwidth250(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Schema name 1
                                                .dropdown-menu
                                                    a.dropdown-item(href='#') Schema 1
                                                    a.dropdown-item(href='#') Schema 2
                                                    a.dropdown-item(href='#') Schema 3
                                            .btn-group.clear.mt-2(style='float:left;')
                                                span.minwidth150.mr-2 Name
                                                input.minwidth250(type='text')
                                            .btn-group.clear.mt-2(style='float:left;')
                                                span.minwidth150.mr-2 Hash Attribute
                                                input.minwidth250(type='text')
                                .modal-footer
                                    .col-md-12.p-0
                                        button.btn.btn-mainadd(type='button', style='min-width:150px;', data-toggle='dropdown', data-dismiss='modal', aria-haspopup='true', aria-expanded='false')
                                            | Add
                    .col-md-4
